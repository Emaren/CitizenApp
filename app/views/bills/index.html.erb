  <div class="container-show">

      <% @bills.each do |bill| %>
      <div class="col-xs-6 col-sm-5 col-md-4 col-lg-3">
          <a href="<%= bill_path(bill)%>">
            <div class="bill-image-link">
              <div class="bill-title"><%= bill.title %></div>
            </div>
          </a>
      </div>
        <% end %>
  </div>
